{
  "name": "08",
  "private": true,
  "version": "8.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "preview": "npm run build && vite preview",
    "deploy": "tsc && vite build --base=/08 --emptyOutDir && wrangler pages deploy ../dist",
    "tsc": "tsc",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint src --fix",
    "lintcss": "stylelint src/**/*.css",
    "lintcss-fix": "stylelint src/**/*.css --fix",
    "test": "vitest run --silent",
    "test-ui": "vitest --ui --open false",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage --silent",
    "validate": "npm test && npm run coverage && npm run lintcss && npm run tsc && npm run lint && npm run tsc",
    "fix": "npm run lintcss-fix && npm run lint-fix",
    "precommit": "commitlint --edit && npm run validate && commitlint --edit",
    "prepare": "husky install && chmod ug+x .husky/*"
  },
  "dependencies": {
    "modern-normalize": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-slick": "^0.23.11",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^10.11.1",
    "husky": "^8.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^10.0.0",
    "type-fest": "^4.3.2",
    "typescript": "^5.1.6",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  }
}
